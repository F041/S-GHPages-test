{
  "openapi": "3.0.3",
  "info": {
    "title": "Stat Model API",
    "description": "An API that gives the prediction of a statistcal model",
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "/S-GHPages-test"
    }
  ],
  "paths": {
    "/predict": {
      "get": {
        "summary": "Add the value for the two variables",
        "parameters": [
          {
            "name": "a",
            "in": "query",
            "required": true,
            "schema": {
              "type": "number"
            },
            "description": "The first variable"
          },
          {
            "name": "b",
            "in": "query",
            "required": true,
            "schema": {
              "type": "number"
            },
            "description": "The second variable"
          }
        ],
        "responses": {
          "200": {
            "description": "Model prediction",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "result": { "type": "number" }
                  }
                }
              }
            }
          },
          "400": {
            "description": "Invalid input",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "error": { "type": "string" }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}
